+   "source": [
+    "n = 10\n",
+    "alpha = 1e-3\n",
+    "w = np.array([1, 0.5])\n",
+    "grad = 1/n * 2 * np.sum(X.T * (np.sum(w * X, axis=1) - y), axis=1)\n",
+    "grad = 1/n * 2 * np.dot(X.T, (np.dot(w, X.T) - y))\n",
+    "w, w - alpha * grad"
